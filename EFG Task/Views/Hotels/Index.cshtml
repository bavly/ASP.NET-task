@model IEnumerable<EFG_Task.Models.Hotel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idInvestor }) |
            @Html.ActionLink("Details", "Details", new { id=item.idInvestor }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idInvestor })
        </td>
    </tr>
}

</table>
